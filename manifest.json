{
  "manifest_version": 3,
  "name": "Dark PDF",
  "version": "1.0.0",
  "description": "A basic extension to add a dark overlay to pdf files",
  "icons": {
    "48": "images/hello-48.png"
  },
  "action": {
    "default_popup": "dpe.html"
  },
  "content_scripts": [
    {
      "js": ["scripts/overlay.js"],
      "matches": ["file:///*"]
    }
  ]
}
